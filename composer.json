{
    "name": "shaulhaf/advanced-nova-media-library-vapor",
    "description": "Laravel Nova tools for managing the Spatie media library.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "^5.6|^6.0|^7.0",
        "laravel/nova": "^2.0|^3.0",
        "spatie/laravel-medialibrary": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ShaulHaf\\AdvancedNovaMediaLibrary\\": "src/",
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShaulHaf\\AdvancedNovaMediaLibrary\\AdvancedNovaMediaLibraryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
